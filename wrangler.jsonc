{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "deepdrama",
  "compatibility_date": "2025-12-16",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "deepdrama-production",
      "database_id": "local-dev"
    }
  ]
}
